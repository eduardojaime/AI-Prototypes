<h1>{{title}}</h1>

<a href="/stories/add" class="btn btn-info">Add a New Story</a>

{{! To Show data }}
<table class="table table-striped table-hover">
  <thead>
    <th>Version</th>
    <th>Status</th>
    <th>Title</th>
    <th>IsShort</th>
    <th>IsFemale</th>
  </thead>
  <tbody>
    {{#each dataset}}
      <tr>
        <td>{{this.Version}}</td>
        <td>{{this.Status}}</td>
        <td>{{this.Title}}</td>
        <td>{{this.IsShort}}</td>
        <td>{{this.IsFemale}}</td>
        <td>
          <a href="/stories/edit/{{this._id}}" class="btn btn-info">Edit</a>
          <a href="/stories/delete/{{this._id}}" onclick="return confirmDeletion()" class="btn btn-danger">Delete</a>
          </td>
      </tr>
    {{/each}}
  </tbody>
</table>